  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.7",
    "live-server": "^1.2.2",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.24"
  }
}
