{
  "name": "blog",
  "version": "1.0.0",
  "description": "Demo blog with tailwind css and html",
  "main": "index.html",
  "scripts": {
    "tailwind": "tailwindcss  -i ./css/main.css -o build/tailwind.css --minify  & live-server  ",
